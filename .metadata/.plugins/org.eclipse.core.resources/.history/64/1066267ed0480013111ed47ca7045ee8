
public class TestaClasseAbstrata {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		Funcionario f = new Estagiario();
		f.setSalario(1000);
		System.out.println(f.getBonificacao());
		
		Conta c = new ContaSalario("diogo");
		c.deposita(100);
		c.atualiza(0.01);
		c.mostra();
		
		Conta c1 = new ContaCorrente();
		c1.mostra();

	}

}
