		Scanner s = new Scanner(System.in);
		PrintStream ps = new PrintStream("texto.txt");
		while(s.hasNextLine() && !s.nextLine().equalsIgnoreCase("fim"))){
			ps.println(s.nextLine());
		}

	}

}
